# Windows Terminal Settings.
mklink "%LOCALAPPDATA%\Packages\Microsoft.WindowsTerminal_8wekyb3d8bbwe\LocalState\settings.json" "%USERPROFILE%\.config\dotfiles\windows\settings.json"

# Symlinks of folders
mklink /J "%UserProfile%\.ssh" "%OneDrive%\Documents\.ssh" 
mklink /J "%UserProfile%\.kube" "%OneDrive%\Documents\.kube"
mklink /J "%UserProfile%\.aws" "%OneDrive%\Documents\.aws" 

# History
mklink "%UserProfile%\.pwsh_history" "%OneDrive%\Documents\.pwsh_history"
mklink "%UserProfile%\.radian_history" "%OneDrive%\Documents\.radian_history"

# Add some icons.
Install-Module -Name Terminal-Icons -Repository PSGallery

